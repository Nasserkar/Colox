{
  "public": true,
  "source": "/:file*.woff2",
  "headers": [
    {
      "key": "Cache-Control",
      "value": "public, max-age=31536000"
    },
    {
      "key": "Access-Control-Allow-Origin",
      "value": "https://colox.vercel.app"
    }
  ],
  "outputDirectory": "build",
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "installCommand": "pnpm install"
}
